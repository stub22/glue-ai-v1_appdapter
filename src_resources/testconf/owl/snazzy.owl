        <rdfs:comment
            >Policies are kinda funny.  Everyone knows we have them, but nobody knows what they are!</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#PresentationJob -->

    <owl:Class rdf:about="#PresentationJob">
        <rdfs:subClassOf rdf:resource="#Job"/>
    </owl:Class>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#Purge -->

    <owl:Class rdf:about="#Purge">
        <rdfs:subClassOf rdf:resource="#Deed"/>
    </owl:Class>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#Query -->

    <owl:Class rdf:about="#Query">
