    </owl:Class>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#Task -->

    <owl:Class rdf:about="#Task">
        <rdfs:subClassOf rdf:resource="#LogicalAbs"/>
        <rdfs:comment
            >Task snapshots may be asserted or proved to be in a TaskStateCategory.   Tasks are resolved into other tasks and jobs.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#TaskStateCategory -->

    <owl:Class rdf:about="#TaskStateCategory">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Complete"/>
                    <rdf:Description rdf:about="#Failed"/>
                    <rdf:Description rdf:about="#Pending"/>
                    <rdf:Description rdf:about="#Running"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#Category"/>
        <rdfs:subClassOf rdf:resource="#Task"/>
    </owl:Class>
    


