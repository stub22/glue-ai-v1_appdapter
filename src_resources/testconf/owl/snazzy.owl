    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#occurrenceTime -->

    <owl:DatatypeProperty rdf:about="#occurrenceTime">
