    <owl:ObjectProperty rdf:about="#result"/>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#sibling -->

    <owl:ObjectProperty rdf:about="#sibling">
        <rdfs:subPropertyOf rdf:resource="#link"/>
    </owl:ObjectProperty>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#spec -->

    <owl:ObjectProperty rdf:about="#spec">
        <rdfs:comment
