    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#condition -->

    <owl:ObjectProperty rdf:about="#condition"/>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#endMark -->

    <owl:ObjectProperty rdf:about="#endMark">
        <rdfs:subPropertyOf rdf:resource="#mark"/>
    </owl:ObjectProperty>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#firstChild -->

    <owl:ObjectProperty rdf:about="#firstChild">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >firstChild has no prevSibling</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#child"/>
    </owl:ObjectProperty>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#host -->

    <owl:ObjectProperty rdf:about="#host"/>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#lastChild -->

    <owl:ObjectProperty rdf:about="#lastChild">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >lastChild has no nextSibling</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#child"/>
    </owl:ObjectProperty>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#link -->

    <owl:ObjectProperty rdf:about="#link">
        <rdfs:comment
            >Updates to related links (e.g. child and sibling) must happen transactionally for correct graph semantics</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#mark -->

    <owl:ObjectProperty rdf:about="#mark"/>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#nextSibling -->

    <owl:ObjectProperty rdf:about="#nextSibling">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <owl:inverseOf rdf:resource="#prevSibling"/>
        <rdfs:subPropertyOf rdf:resource="#sibling"/>
    </owl:ObjectProperty>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#postCond -->

    <owl:ObjectProperty rdf:about="#postCond">
        <rdfs:subPropertyOf rdf:resource="#condition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#preCond -->

    <owl:ObjectProperty rdf:about="#preCond">
        <rdfs:comment
            >often points to a policy</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#condition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.appdapter.org/ontologies/2010/snazzy.owl#prevSibling -->

    <owl:ObjectProperty rdf:about="#prevSibling">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
